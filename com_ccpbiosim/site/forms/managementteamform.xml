<?xml version="1.0" encoding="utf-8"?>
<form addfieldprefix="Ccpbiosim\Component\Ccpbiosim\Site\Field">
  <fieldset>
    <field name="id" type="text" readonly="true" class="readonly" default="0" description="JGLOBAL_FIELD_ID_DESC"/>
    <field name="state" default="1" type="list" label="JSTATUS" description="JFIELD_PUBLISHED_DESC" class="inputbox" size="1">
      <option value="1">JPUBLISHED</option>
      <option value="0">JUNPUBLISHED</option>
      <option value="2">JARCHIVED</option>
      <option value="-2">JTRASHED</option>
    </field>
    <field name="ordering"/>
    <field name="checked_out" type="hidden" filter="unset"/>
    <field name="checked_out_time" type="hidden" filter="unset"/>
    <field name="title" type="list" required="true" label="JGLOBAL_TITLE" description="COM_CCPBIOSIM_FORM_DESC_MANAGEMENTTEAM_TITLE" hint="COM_CCPBIOSIM_FORM_LBL_MANAGEMENTTEAM_TITLE">
      <option value="mr">COM_CCPBIOSIM_MANAGEMENTTEAMS_TITLE_OPTION_MR</option>
      <option value="mrs">COM_CCPBIOSIM_MANAGEMENTTEAMS_TITLE_OPTION_MRS</option>
      <option value="miss">COM_CCPBIOSIM_MANAGEMENTTEAMS_TITLE_OPTION_MISS</option>
      <option value="doctor">COM_CCPBIOSIM_MANAGEMENTTEAMS_TITLE_OPTION_DOCTOR</option>
      <option value="prof">COM_CCPBIOSIM_MANAGEMENTTEAMS_TITLE_OPTION_PROF</option>
    </field>
    <field name="firstname" maxlength="30" filter="safehtml" type="text" required="true" label="COM_CCPBIOSIM_FORM_LBL_MANAGEMENTTEAM_FIRSTNAME" description="COM_CCPBIOSIM_FORM_DESC_MANAGEMENTTEAM_FIRSTNAME" hint="First name of team member"/>
    <field name="surname" maxlength="30" filter="safehtml" type="text" required="true" label="COM_CCPBIOSIM_FORM_LBL_MANAGEMENTTEAM_SURNAME" description="COM_CCPBIOSIM_FORM_DESC_MANAGEMENTTEAM_SURNAME" hint="Surname of team member"/>
    <field name="role" filter="safehtml" type="textarea" required="true" label="COM_CCPBIOSIM_FORM_LBL_MANAGEMENTTEAM_ROLE" description="COM_CCPBIOSIM_FORM_DESC_MANAGEMENTTEAM_ROLE" hint="Team member role"/>
    <field name="profilephoto" types="images" directory="people" type="media" label="COM_CCPBIOSIM_FORM_LBL_MANAGEMENTTEAM_PROFILEPHOTO" description="COM_CCPBIOSIM_FORM_DESC_MANAGEMENTTEAM_PROFILEPHOTO" hint="COM_CCPBIOSIM_FORM_LBL_MANAGEMENTTEAM_PROFILEPHOTO"/>
    <field name="groupwebsite" validate="url" type="url" label="COM_CCPBIOSIM_FORM_LBL_MANAGEMENTTEAM_GROUPWEBSITE" description="COM_CCPBIOSIM_FORM_DESC_MANAGEMENTTEAM_GROUPWEBSITE" hint="COM_CCPBIOSIM_FORM_LBL_MANAGEMENTTEAM_GROUPWEBSITE"/>
    <field name="social" validate="url" type="url" label="COM_CCPBIOSIM_FORM_LBL_MANAGEMENTTEAM_SOCIAL" description="COM_CCPBIOSIM_FORM_DESC_MANAGEMENTTEAM_SOCIAL" hint="COM_CCPBIOSIM_FORM_LBL_MANAGEMENTTEAM_SOCIAL"/>
    <field name="chair" type="checkbox" checked="" label="COM_CCPBIOSIM_FORM_LBL_MANAGEMENTTEAM_CHAIR" description="COM_CCPBIOSIM_FORM_DESC_MANAGEMENTTEAM_CHAIR" hint="COM_CCPBIOSIM_FORM_LBL_MANAGEMENTTEAM_CHAIR"/>
    <field name="cosecprojectlead" type="checkbox" checked="" label="COM_CCPBIOSIM_FORM_LBL_MANAGEMENTTEAM_COSECPROJECTLEAD" description="COM_CCPBIOSIM_FORM_DESC_MANAGEMENTTEAM_COSECPROJECTLEAD" hint="COM_CCPBIOSIM_FORM_LBL_MANAGEMENTTEAM_COSECPROJECTLEAD"/>
    <field name="adminassistant" type="checkbox" checked="" label="COM_CCPBIOSIM_FORM_LBL_MANAGEMENTTEAM_ADMINASSISTANT" description="COM_CCPBIOSIM_FORM_DESC_MANAGEMENTTEAM_ADMINASSISTANT" hint="COM_CCPBIOSIM_FORM_LBL_MANAGEMENTTEAM_ADMINASSISTANT"/>
    <field name="insitution" filter="safehtml" type="text" label="COM_CCPBIOSIM_FORM_LBL_MANAGEMENTTEAM_INSITUTION" description="COM_CCPBIOSIM_FORM_DESC_MANAGEMENTTEAM_INSITUTION" hint="Members institution"/>
    <field name="secretary" type="checkbox" checked="" label="COM_CCPBIOSIM_FORM_LBL_MANAGEMENTTEAM_SECRETARY" description="COM_CCPBIOSIM_FORM_DESC_MANAGEMENTTEAM_SECRETARY" hint="COM_CCPBIOSIM_FORM_LBL_MANAGEMENTTEAM_SECRETARY"/>
    <field name="version_note" type="text" label="JGLOBAL_FIELD_VERSION_NOTE_LABEL" description="JGLOBAL_FIELD_VERSION_NOTE_DESC" class="inputbox" size="45" labelclass="control-label"/>
  </fieldset>
</form>
